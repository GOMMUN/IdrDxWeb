<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.idr.pdd.mapper.FactoryMapper">

    <select id="find" resultType="Factory">
        SELECT 
        	COMPANYID, 
        	FACTORYID, 
        	FACTORYNAME, 
        	DESCRIPTION, 
        	ERPPLANT, 
        	FACTORYTYPE, 
        	CREATOR, 
        	CREATETIME, 
        	EVENT, 
        	EVENTUSER, 
        	EVENTTIME, 
        	ISUSABLE, 
        	TID 
        FROM DSP_FACTORY
    </select>

</mapper>
