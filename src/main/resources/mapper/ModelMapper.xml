<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.idr.pdd.mapper.ModelMapper">


	<select id="modelfind" resultType="ModelNM">
		SELECT
		MODEL_ID AS MODELID,
		MODEL_NM AS MODELNAME
		FROM CM0501
		WHERE 1=1
	</select>

</mapper>
